<div class="main users-new">
  <div class="container">
    <div class="form-heading">ログイン</div>
     <% if @error_message %>
      <div class = "form-error">
       <%= @error_message %>
      </div>
     <% end %>
    <%= form_with url:login_path, local: true do |form| %>
        <%= form.label :メールアドレス %>
        <li>
        <%= form.text_field :email %>
        </li>
        <%= form.label :パスワード %>
        <li>
        <%= form.password_field :password %>
        </li>
        <%= form.submit "ログイン" %>
        <% end %>
    </div>
  </div>
</div>