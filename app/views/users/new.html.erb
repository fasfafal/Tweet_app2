<div class="main users-new">
  <div class="container">
   
    <%= form_with(model: @user, local:true) do |f| %>
        <div><%= f.label :"新規ユーザー登録"  %></div>
        <div><%= f.text_field :name %></div>
        <div><%= f.label :"メールアドレス" %></div>
        <div><%= f.text_field :email %></div>
        <div><%= f.submit "新規登録" %></div>
    <% end %>
  </div>
</div>